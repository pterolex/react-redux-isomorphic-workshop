{
  "name": "Films-Library",
  "version": "0.1.0",
  "description": "Films library",
  "author": {
    "name": "Alexey Zaslavsky",
    "email": "office@webbylab.com",
    "url": "http://webbylab.com"
  },
  "private": true,
  "scripts": {
    "build": "NODE_ENV='production' webpack -p",
    "start": "webpack -d --watch --colors",
    "server": "nodemon --exec babel-node -- server/app.js",
    "devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8050 --host 0.0.0.0",
    "lint": "eslint shared client server",
    "test": "npm run lint",
    "po2json": "./bin/po2json.sh",
    "start": "NODE_ENV='production' pm2 start server/app.js --interpreter node_modules/babel/bin/babel-node.js --name itsquiz-wall",
    "stop": "NODE_ENV='production' pm2 delete itsquiz-wall -s &> /dev/null"
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "classnames": "^2.1.5",
    "express": "^4.13.3",
    "history": "^1.12.4",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "^2.1.4",
    "moment": "^2.10.6",
    "query-string": "^3.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-mdl": "^0.15.0",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.3",
    "redux-router": "^0.1.0",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "*",
    "babel": "5.*",
    "babel-core": "5.*",
    "babel-eslint": "^4.1.1",
    "babel-loader": "5.*",
    "babel-plugin-react-autoprefix": "^0.2.6",
    "css-loader": "^0.22.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "nodemon": "^1.8.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  }
}
